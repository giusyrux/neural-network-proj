%% Parametri
params;

%% Estrazione dataset
files = ['mnist/t10k-images-idx3-ubyte' 'mnist/t10k-labels-idx1-ubyte'];
[X,T] = prepareDataset(files);

%% SHUFFLE
ind=randperm(size(X,2));
X=X(:,ind);
T=T(:,ind);

%% SPLIT TRAIN, VALIDATION, TEST SET
XTrain= X(:,1:size(X,2);
TTrain= T(:,1:5000);

XVal=X(:,5001:7500);
TVal= T(:,5001:7500);

XTest=X(:,7501:end);
TTest= T(:,7501:end);